{
    "manifest_version": 2,
    "name": "fastcadastrex",
    "version": "0.0.1",
    "applications": {
        "gecko": {
            "id": "fastcadastrex@tolokoban.org"
        }
    },

    "background": {
        "scripts": ["index.js"],
        "persistent": false
    },

    "permissions": [
        "tabs"
    ],

    "icons": {
        "16": "icon-16.png",
        "32": "icon-32.png",
        "64": "icon-64.png"
    },

    "default_icon": {
        "16": "icon-16.png",
        "32": "icon-32.png",
        "64": "icon-64.png"
    },

    "browser_action": {
        "default_title": "Cadastre Xavier"
    }
}
